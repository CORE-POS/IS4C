        }
    },
    "require": {
        "components/jquery": "1.11.1",
        "components/bootstrap": "3.2.*",
        "components/bootstrap-default": "~3.2",
        "adodb/adodb-php": "dev-master",
        "core-pos/sql-manager": "dev-master",
        "php-xpdf/php-xpdf" : "dev-master",
        "phpmailer/phpmailer": "~5.2",
        "psr/log": "~1.0",
        "sabre/vobject": "~3.4",
        "videlalvaro/php-amqplib": "~2.5",
        "endroid/qrcode": "~1.5",
        "itbz/fpdf": "~1.7"
